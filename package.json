{
  "name": "node-extra-funcs",
  "version": "1.0.1",
  "engines": {
    "node": ">=3.3.1",
    "npm": ">=2.14.3"
  },
  "description": "A string library that adds additional functions to the String object.",
  "main": "index.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^2.13.0",
    "mocha": "^2.5.3",
    "ink-docstrap": "^1.2.1",
    "jsdoc": "^3.4.0"
  },
  "scripts": {
    "test": "./bin/run_tests.sh",
    "docpublish": "./bin/build_docs.sh && ./bin/publish_docs.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eledger-org/node-extra-funcs.git"
  },
  "author": "Henry Brown",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eledger-org/node-extra-funcs/issues"
  },
  "homepage": "https://github.com/eledger-org/node-extra-funcs#readme",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "comma-dangle": "warn",
      "semi": "error",
      "curly": "error",
      "quotes": [
        "error",
        "double"
      ],
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ]
    }
  }
}
